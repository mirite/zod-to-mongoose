{
    "name": "@mirite/zod-to-mongoose",
    "version": "0.0.1",
    "description": "Package for creating mongoose schemas out of Zod schemas for type safety, and removing duplicate effort.",
    "types": "dist/_tsup-dts-rollup.d.ts",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mirite/zod-to-mongoose.git"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "author": {
        "name": "Jesse Conner",
        "email": "me@jesseconner.ca",
        "url": "https://jesseconner.ca"
    },
    "keywords": [
        "zod",
        "mongoose",
        "schema",
        "type",
        "type-safe"
    ],
    "issues": "https://github.com/mirite/zod-to-mongoose/issues",
    "license": "MIT",
    "scripts": {
        "build": "tsup",
        "test": "jest --coverage",
        "lint": "eslint . --fix --ignore-path .gitignore",
        "format": "prettier --write . --ignore-path .gitignore",
        "release": "yarn build && npm publish"
    },
    "devDependencies": {
        "@types/eslint": "^8.56.2",
        "@types/jest": "^29.5.12",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^7.0.1",
        "eslint": "^8.56.0",
        "eslint-plugin-jsdoc": "^48.0.6",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "prettier-plugin-jsdoc": "^1.3.0",
        "ts-jest": "^29.1.2",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3"
    },
    "packageManager": "yarn@4.1.0",
    "dependencies": {
        "@microsoft/api-extractor": "^7.40.1",
        "mongoose": "^8.1.2",
        "zod": "^3.22.4"
    }
}
